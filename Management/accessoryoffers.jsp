<script type="text/javascript">

	function doAccountAction(parameter) {
		if(loginState == 'loggedIn'){
			location.href = getAccountActionLink(parameter);
		}else {
			callSignInOverlay(parameter);
		}
	}

	function callSignInOverlay(parameter) {
		var destination = getAccountActionLink(parameter);

		var specObj = {LButLabel:'Sign In',LButURL:destination};
		signInDialog.render("", specObj);
	}

	function getAccountActionLink(parameter) {
		var destination = myaccountURL;
		switch (parameter)
		{
			case "addALine":
				destination = secureapplicationserver + '/b2c/accountholder/estore/addaline';
				break;
			case "upgradeYourDevice":
				destination = secureapplicationserver + '/b2c/accountholder/estore/phoneupgrade';
				break;
			case "accessoryOnly":
				destination = secureapplicationserver + '/b2c/store/accessories';
				break;
			default:
				destination = myaccountURL;
		}
		return destination;
	}

</script>

